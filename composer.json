{
    "name": "emiljimenez21/laravel-jwt-auth",
    "description": "A JWT client for applications behind an OIDC or OAuth2.0 identity provider",
    "keywords": [
        "emiljimenez21",
        "laravel-jwt-auth"
    ],
    "homepage": "https://github.com/emiljimenez21/laravel-jwt-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Emil Jimenez",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "firebase/php-jwt": "^6.10",
        "illuminate/support": "^7.20|^8.19|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "EmilJimenez21\\JWTAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmilJimenez21\\JWTAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmilJimenez21\\JWTAuth\\JwtAuthServiceProvider"
            ]
        }
    }
}
